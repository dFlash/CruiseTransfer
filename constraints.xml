<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:batch="http://www.springframework.org/schema/batch" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="fkCityCountry" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="city" />
		<property name="targetTable" value="country" />
		<property name="sourceColumn" value="country_id" />
		<property name="targetColumn" value="country_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE RESTRICT" />
	</bean>
	
	<bean id="fkCityGallery" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="city" />
		<property name="targetTable" value="gallery" />
		<property name="sourceColumn" value="gallery_id" />
		<property name="targetColumn" value="gallery_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE SET NULL" />
	</bean>
	
	<bean id="fkCountryGallery" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="country" />
		<property name="targetTable" value="gallery" />
		<property name="sourceColumn" value="gallery_id" />
		<property name="targetColumn" value="gallery_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE SET NULL" />
	</bean>
	
	<bean id="fkCompanyGallery" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="company" />
		<property name="targetTable" value="gallery" />
		<property name="sourceColumn" value="gallery_id" />
		<property name="targetColumn" value="gallery_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE SET NULL" />
	</bean>
	
	<bean id="fkCompanyCompanyCat" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="company" />
		<property name="targetTable" value="company_category" />
		<property name="sourceColumn" value="company_category_id" />
		<property name="targetColumn" value="company_category_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE RESTRICT" />
	</bean>
	
	<bean id="fkCompanyCruiseCat" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="company" />
		<property name="targetTable" value="cruise_category" />
		<property name="sourceColumn" value="company_cruise_category_id" />
		<property name="targetColumn" value="cruise_category_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE NO ACTION" />
	</bean>
	
	<bean id="fkCruiseCruiseCat" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="cruise" />
		<property name="targetTable" value="cruise_category" />
		<property name="sourceColumn" value="cruise_category_id" />
		<property name="targetColumn" value="cruise_category_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE RESTRICT" />
	</bean>
	
	<bean id="fkCruiseLanguageService" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="cruise" />
		<property name="targetTable" value="cruise_language_service" />
		<property name="sourceColumn" value="cruise_language_service_id" />
		<property name="targetColumn" value="cruise_language_service_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE SET NULL" />
	</bean>
	
	<bean id="fkCruiseDescriptionGallery" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="cruise" />
		<property name="targetTable" value="gallery" />
		<property name="sourceColumn" value="description_gallery_id" />
		<property name="targetColumn" value="gallery_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE SET NULL" />
	</bean>
	
	<bean id="fkCruiseGallery" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="cruise" />
		<property name="targetTable" value="gallery" />
		<property name="sourceColumn" value="gallery_id" />
		<property name="targetColumn" value="gallery_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE SET NULL" />
	</bean>
	
	<bean id="fkCruiseVessel" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="cruise" />
		<property name="targetTable" value="vessel" />
		<property name="sourceColumn" value="vessel_id" />
		<property name="targetColumn" value="vessel_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE CASCADE" />
	</bean>
	
	<bean id="fkCruiseRegion" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="cruise" />
		<property name="targetTable" value="region" />
		<property name="sourceColumn" value="region_id" />
		<property name="targetColumn" value="region_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE SET NULL" />
	</bean>
	
	<bean id="fkCruiseDateRangeCruise" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="cruise_date_range" />
		<property name="targetTable" value="cruise" />
		<property name="sourceColumn" value="cruise_id" />
		<property name="targetColumn" value="cruise_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE CASCADE" />
	</bean>
	
	<bean id="fkCruiseCityCity" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="cruise_city" />
		<property name="targetTable" value="city" />
		<property name="sourceColumn" value="city_id" />
		<property name="targetColumn" value="city_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE RESTRICT" />
	</bean>
	
	<bean id="fkCruiseCityCruise" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="cruise_city" />
		<property name="targetTable" value="cruise" />
		<property name="sourceColumn" value="cruise_id" />
		<property name="targetColumn" value="cruise_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE CASCADE" />
	</bean>
	
	<bean id="fkFileGallery" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="file" />
		<property name="targetTable" value="gallery" />
		<property name="sourceColumn" value="gallery_id" />
		<property name="targetColumn" value="gallery_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE CASCADE" />
	</bean>
	
	<bean id="fkGalleryFile" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="gallery" />
		<property name="targetTable" value="file" />
		<property name="sourceColumn" value="default_file_id" />
		<property name="targetColumn" value="file_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE SET NULL" />
	</bean>
	
	<bean id="fkVesselCompany" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="vessel" />
		<property name="targetTable" value="company" />
		<property name="sourceColumn" value="company_id" />
		<property name="targetColumn" value="company_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE RESTRICT" />
	</bean>
	
	<bean id="fkVesselDescGallery" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="vessel" />
		<property name="targetTable" value="gallery" />
		<property name="sourceColumn" value="description_gallery_id" />
		<property name="targetColumn" value="gallery_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE SET NULL" />
	</bean>
	
	<bean id="fkVesselGallery" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="vessel" />
		<property name="targetTable" value="gallery" />
		<property name="sourceColumn" value="gallery_id" />
		<property name="targetColumn" value="gallery_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE SET NULL" />
	</bean>
	
	<bean id="fkVesselSchemaGallery" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="vessel" />
		<property name="targetTable" value="gallery" />
		<property name="sourceColumn" value="schema_gallery_id" />
		<property name="targetColumn" value="gallery_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE SET NULL" />
	</bean>
	
	<bean id="fkVesselVesselCategory" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="vessel" />
		<property name="targetTable" value="vessel_category" />
		<property name="sourceColumn" value="vessel_category_id" />
		<property name="targetColumn" value="vessel_category_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE RESTRICT" />
	</bean>
	
	<bean id="fkContinentGallery" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="continent" />
		<property name="targetTable" value="gallery" />
		<property name="sourceColumn" value="gallery_id" />
		<property name="targetColumn" value="gallery_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE SET NULL" />
	</bean>
	
	<bean id="fkRegionGallery" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="region" />
		<property name="targetTable" value="gallery" />
		<property name="sourceColumn" value="gallery_id" />
		<property name="targetColumn" value="gallery_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE SET NULL" />
	</bean>
	
	<bean id="fkRegionContinent" class="net.maliavin.transfer.config.FkConstraint">
		<property name="sourceTable" value="region" />
		<property name="targetTable" value="continent" />
		<property name="sourceColumn" value="continent_id" />
		<property name="targetColumn" value="continent_id" />
		<property name="condition" value="ON UPDATE NO ACTION ON DELETE RESTRICT" />
	</bean>

	<bean id="constraints" class="java.util.ArrayList">
		<constructor-arg>
			<list value-type="net.maliavin.transfer.config.FkConstraint">
				<ref bean="fkCityCountry" />
				<ref bean="fkCityGallery" />
				<ref bean="fkCountryGallery" />
				<ref bean="fkCompanyGallery" />
				<ref bean="fkCompanyCompanyCat" />
				<ref bean="fkCompanyCruiseCat" />
				<ref bean="fkCruiseCruiseCat" />
				<ref bean="fkCruiseLanguageService" />
				<ref bean="fkCruiseDescriptionGallery" />
				<ref bean="fkCruiseGallery" />
				<ref bean="fkCruiseVessel" />
				<ref bean="fkCruiseRegion" />
				<ref bean="fkCruiseDateRangeCruise" />
				<ref bean="fkCruiseCityCity" />
				<ref bean="fkCruiseCityCruise" />
				<ref bean="fkFileGallery" />
				<ref bean="fkGalleryFile" />
				<ref bean="fkVesselCompany" />
				<ref bean="fkVesselDescGallery" />
				<ref bean="fkVesselGallery" />
				<ref bean="fkVesselSchemaGallery" />
				<ref bean="fkVesselVesselCategory" />
				<ref bean="fkContinentGallery" />
				<ref bean="fkRegionGallery" />
				<ref bean="fkRegionContinent" />
			</list>
		</constructor-arg>
	</bean>

</beans>